SELECT
  TIPO,
  SUCURSAL,
  CANAL,
  ORIGEN,
  ENCARGADO_TURNO,
  CAJERO,
  IDTURNO,
  IDCONTROLCAJA,
  APERTURA,
  IDCOMANDA,
  PEDIDO,
  IDMESA,
  TOTAL,
  ANULADO,
  ORIGINAL,
  FECHA_ANULACION,
  IDOPERACION,
  OPERACION,
  AUTORIZACION,
  MOTIVO,
  USUARIO,
  VENDEDOR
FROM
  SP_GG_ANULACIONES_EH(
  :DESDE_1,
  :HASTA_1,
  :IDSUCURSAL_1)
WHERE 
  APERTURA >= '06/01/2025'
ORDER BY
  APERTURA
