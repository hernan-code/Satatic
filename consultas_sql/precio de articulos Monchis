SELECT a.IDARTICULO ,a.DESCRIP_APPS,p.precio,p2.descripcion AS origen
FROM ARTICULO a 
INNER JOIN PRECIO p ON
	a.IDARTICULO = p.IDARTICULO
INNER JOIN PLANVENTA p2 ON
	p.IDPLANVENTA = p2.IDPLANVENTA
INNER JOIN PLANVENTA pl ON p.IDPLANVENTA = pl.IDPLANVENTA
WHERE p2.descripcion = 'MONCHIS'
AND a.DESCRIP_APPS LIKE '%MINI%'
ORDER BY p.PRECIO 
